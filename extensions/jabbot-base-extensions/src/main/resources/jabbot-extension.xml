<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean class="org.wanna.jabbot.command.HelpCommand">
        <constructor-arg value="help"/>
    </bean>

    <bean class="org.wanna.jabbot.command.VersionCommand">
        <constructor-arg value="version"/>
    </bean>

    <bean class="org.wanna.jabbot.command.UptimeCommand">
        <constructor-arg value="uptime"/>
    </bean>
</beans>